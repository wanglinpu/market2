/* 头部开始 */
page {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.custom_mad {
  width: 100%;
  height: 508rpx;
  background-color: #FBFBFB;
  position: relative;
}
.custom_mad image {
  width: 100%;
  height: 100%;
}
.custom_tab {
  font-size: 24rpx;
  position: absolute;
  bottom: 82rpx;
  right: 0;
  padding-right: 0;
}
.custom_tab view {
  text-align: right;
  height: 50rpx;
}
.custom_tab view:last-child {
  height: 36rpx;
}
.information {
  width: 100%;
  height: 80rpx;
  border-top: 2rpx solid #41C7D6;
  border-bottom: 2rpx solid #41C7D6;
  line-height:80rpx; 
  position: absolute;
  bottom: 0;
  background-color: #FDFDAD; 
}
.custom_mad .name {
  font-size: 28rpx;
  color: #000;

}
.custom_mad text {
  margin-left: 28rpx;
  color: #000;
}
.custom_mad .duty {
  font-size: 24rpx;
  color: #000;
}
.custom_mad .number {
  font-size: 24rpx;
  color: #000;
}
/* 头部结束 */
/* 主体部分开始 */
.main {
  width: 100%; 
  padding-top: 12rpx;
}
.main navigator {
 height: 120rpx;
 line-height: 120rpx;
 font-size: 24rpx;
 border-bottom: 1rpx solid #eee;
}

